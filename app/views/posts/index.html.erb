<h1>My Posts</h1>
<%= link_to 'New Post', new_post_path %>
<% @posts.each do |post| %>
  <ul>
    <li><%= post.title %> | <%= link_to 'Go', post_path(post) %> <%= link_to 'Edit', edit_post_path(post) %> <%= link_to 'Delete', post_path(post), method: :delete, data: {confirm: 'Are you sure you want to delete this post?'} %></li>
  </ul>
<% end %>